  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^9.7.3",
    "json-server": "^0.15.1",
    "postcss-cli": "^6.1.3",
    "tailwindcss": "^1.1.4"
  }
